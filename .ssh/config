# Global

# Only use given identity file
IdentitiesOnly Yes

# Include specifics config files
Include config.d/*

Host gitlab.com
    User git
    Identityfile ~/.ssh/gitlab.com_gitlab-user_cipher

Host github.com
    User git
    Identityfile ~/.ssh/github.com_github-user_cipher

Host notabug.org
    User git
    Identityfile ~/.ssh/notabug.org_notabug-user_cipher
